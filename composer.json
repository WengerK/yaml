{
    "name": "dallgoot/yaml",
    "description": "Provides loader, dumper and an API for YAML files. Loader builds to equivalent data types in PHP7",
    "type": "library",
    "license": "Apache-2.0",
    "config": {
        "vendor-dir":"dependencies"
    },
    "authors": [
        {
            "name": "dallgoot",
            "email": "stephane.rebai@gmail.com"
        }
    ],
      "autoload": {
        "classmap": [
          "sources",
          "tests",
          "dependencies"
        ]
    },
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-pcre": "*",
        "ext-Reflection": "*",
        "ext-SPL": "*",
        "lib-pcre": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "phan/phan": "^1.2",
        "symfony/yaml": "^4.2",
        "phpmetrics/phpmetrics": "^2.4"
    }
}
