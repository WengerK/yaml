{
    "name": "dallgoot/yaml",
    "description": "Provides loader, dumper and an API for YAML files. Loader builds to equivalent data types in PHP7",
    "type": "library",
    "license": "Apache-2.0",
    "config": {
        "vendor-dir":"dependencies"
    },
    "authors": [
        {
            "name": "dallgoot",
            "email": "stephane.rebai@gmail.com"
        }
    ],
      "autoload": {
        "classmap": [
          "source",
          "tests",
          "dependencies"
        ]
    },
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-pcre": "*",
        "ext-Reflection": "*",
        "ext-SPL": "*",
        "lib-pcre": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "symfony/yaml": "^4.2",
        "symfony/yaml": "^3.4.9",
        "jetbrains/phpstorm-stubs": "dev-master",
        "felixfbecker/language-server": "^5.4",
        "apigen/apigen": "dev-master",
        "roave/better-reflection": "dev-master#c87d856"
    }
}
